{
  "presets": [
    "@babel/preset-typescript",
    "@babel/preset-react",
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "node": "6.10"
      }
    }]
  ],
  "plugins": [
    ["@babel/transform-runtime"],
    ["babel-plugin-styled-components"],
    ["import", {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true
    }, "antd"],
    ["import", {
      "libraryName": "@ant-design/icons",
      // "style": false,
      "libraryDirectory": "es/icons",
      "camel2DashComponentName": false
    }, "@ant-design/icons"],
    ["import", {
      "libraryName": "antd-mobile"
    }, "antd-mobile"],
    ["import", {
      "libraryName": "lodash"
    }, "lodash"]
  ]
}